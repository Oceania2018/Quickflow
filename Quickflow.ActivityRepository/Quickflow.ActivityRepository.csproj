<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/Haiping-Chen/Quickflow</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Haiping-Chen/Quickflow</RepositoryUrl>
    <Authors>Haiping Chen</Authors>
    <Company>Personal</Company>
    <Product>Quickflow</Product>
    <PackageTags>Quickflow CustomEntityFoundation Workflow</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Description>Quickflow activity repository. Integrated with CustomEntityFoundation.</Description>
    <Version>1.5.1</Version>
    <PackageReleaseNotes>Bug fix.</PackageReleaseNotes>
    <AssemblyVersion>1.5.1.0</AssemblyVersion>
    <FileVersion>1.5.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.SimpleEmail" Version="3.3.6.1" />
    <PackageReference Include="RazorLight" Version="2.0.0-beta1" />
    <PackageReference Include="RestSharp" Version="106.2.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Quickflow.Core\Quickflow.Core.csproj" />
  </ItemGroup>

</Project>
