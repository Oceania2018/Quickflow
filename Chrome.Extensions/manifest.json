{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [ "activeTab", "declarativeContent", "storage", "https://*/" ],
  "background": {
    "scripts": [ "signalr.min.js", "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}